{
    "name": "crcms/crcms",
    "description": "The CrCms Framework.",
    "keywords": ["crcms", "laravel"],
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "crcms/framework": "dev-master",
        "crcms/user": "*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
        ],
        "psr-4": {
            "CrCms\\Extensions\\": "extensions/",
            "CrCms\\Modules\\": "modules/"
        },
        "files": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php bin/artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php bin/artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform":{
            "php":"7.1.12"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.laravel-china.org"
        },
        "0": {
            "type": "path",
            "url": "../packages/module"
        },
        "2": {
            "type": "path",
            "url": "../packages/category"
        },
        "3": {
            "type": "path",
            "url": "../packages/user"
        }
    }
}
